modules = ["web", "python-3.11", "nodejs-20", "postgresql-16", "java-graalvm22.3"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "freetype", "glibcLocales", "libiconv", "openssl", "postgresql", "rustc", "imagemagick", "openjdk17", "unzip"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "POS Application"

[[workflows.workflow]]
name = "POS Application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

